{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "Faq"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "question": {
      "type": "text",
      "maxLength": 500,
      "required": true
    },
    "answer": {
      "type": "blocks",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "default": "Getting Started",
      "enum": [
        "Getting Started",
        "Messaging",
        "Account",
        "Privacy",
        "Technical"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "is_published": {
      "type": "boolean",
      "default": false
    }
  }
}
